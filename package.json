{
  "name": "images-service",
  "version": "",
  "description": "",
  "author": "",
  "license": "",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "build:grunt": "grunt",
    "dev:client": "webpack -d --watch",
    "dev:server": "webpack --mode=development",
    "prod:server": "NODE_ENV=production webpack --mode=production",
    "debug": "nodemon --inspect server/server.js",
    "test": "jest -e --forceExit --verbose --updateSnapshot --coverage",
    "lint": "eslint ./",
    "seed:mongo:json": "node server/database/seed/seed1.js & node server/database/seed/seed2.js & node server/database/seed/seed3.js & node server/database/seed/seed4.js;",
    "seed:mongo": "mongoimport --db images --collection images --file ./server/database/seed/images1.json & mongoimport --db images --collection images --file ./server/database/seed/images2.json & mongoimport --db images --collection images --file ./server/database/seed/images3.json & mongoimport --db images --collection images --file ./server/database/seed/images4.json",
    "seed:mongo:delete": "rm server/database/seed/json/images1.json; rm server/database/seed/json/images2.json; rm server/database/seed/json/images3.json; rm server/database/seed/json/images4.json;",
    "bombard": "artillery run ./stressTesting/artillery.yml"
  },
  "dependencies": {
    "aws-sdk": "^2.230.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "grunt-webpack": "^3.1.1",
    "isomorphic-style-loader": "^4.0.0",
    "lodash": "^4.17.5",
    "mongoose": "^5.0.14",
    "newrelic": "^4.1.0",
    "npm": "^6.0.0",
    "path": "^0.12.7",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-slick": "^0.23.1",
    "react-transition-group": "^2.3.1",
    "redis": "^2.8.0",
    "request": "^2.85.0",
    "response-time": "^2.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "grunt": "^1.0.2",
    "grunt-aws": "^0.6.2",
    "grunt-contrib-uglify": "^3.3.0",
    "jest": "^22.4.3",
    "mocha": "^5.0.5",
    "postcss-loader": "^2.1.5",
    "react-test-renderer": "^16.3.1",
    "sinon": "^4.5.0",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.js?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/client/test/__mocks__/styleMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./testSetUp.js"
    ]
  }
}
